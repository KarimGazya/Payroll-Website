    <link rel="stylesheet" href="css\style.css" />
    <title>Department</title>
  </head>
  <body>
    <form action="Department.ejs" method="post">
      <div class="card main container text-center bg-dark text-light p-4">
        <div class="card-body">
          <div class="row alert">
            <%=message%>
          </div>
          <div class="row">
            <h1 class="text-left mb-4">Add Department</h1>
          </div>
          
        
        <div class="row mt-3">
            <div class="input-group">
              <input
                class="password-field bg-dark form-control text-light"
                type="text"
                placeholder="Department Name"
                autocomplete="off"
                name="Name"
                required
                autofocus
              />
            </div>
          </div>
          <div class="row">
            <div class="col-md-6 col-sm-12">
              <a class="btn btn-success mt-5 btn-block font-weight-bold" href="/">Return back</a>
            </div>
            <div class="col-md-6 col-sm-12">
              <button class="btn btn-success mt-5 btn-block font-weight-bold">
                ADD
               </button>
            </div>
          </div>
        

          
      
    </form>
        </div>
        

    <script
      src="https://code.jquery.com/jquery-3.6.0.js"
